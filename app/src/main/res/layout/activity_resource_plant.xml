<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".resourcePlantActivity">


    <ImageView
        android:id="@+id/back_normal_plant2"
        android:layout_width="22dp"
        android:layout_height="29.23dp"
        android:layout_alignParentLeft="true"
        android:layout_alignParentTop="true"
        android:layout_marginStart="16dp"
        android:background="@drawable/ic_left_arrow_24dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toStartOf="@+id/textView"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.022"
        tools:ignore="MissingConstraints" />

    <TextView
        android:id="@+id/textView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentLeft="true"
        android:layout_alignParentTop="true"
        android:layout_marginEnd="256dp"
        android:text="자원관리"
        android:textColor="@color/black"
        android:textSize="30sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.015"
        tools:ignore="MissingConstraints" />

    <ImageView
        android:id="@+id/powerPlant2"
        android:layout_width="100dp"
        android:layout_height="100dp"
        android:layout_alignParentLeft="true"
        android:layout_alignParentTop="true"
        android:layout_marginTop="64dp"
        android:background="@drawable/img_powerplant_green"
        app:layout_constraintBottom_toTopOf="@+id/PlantName"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.472"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        tools:ignore="MissingConstraints,UseAppTint" />

    <TextView
        android:id="@+id/PlantName"
        android:layout_width="100dp"
        android:layout_height="33dp"
        android:layout_alignParentLeft="true"
        android:layout_alignParentTop="true"
        android:layout_marginBottom="52dp"
        android:gravity="center_horizontal|top"
        android:text="**발전소"
        android:textColor="@color/black"
        android:textSize="24sp"
        app:layout_constraintBottom_toTopOf="@+id/rectangle_normal"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.472"
        app:layout_constraintStart_toStartOf="parent"
        tools:ignore="MissingConstraints" />

    <ImageView
        android:id="@+id/rectangle_normal"
        android:layout_width="361dp"
        android:layout_height="323dp"
        android:layout_alignParentLeft="true"
        android:layout_alignParentTop="true"
        android:alpha="0.25"
        android:background="@drawable/img_rectangle_normal"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.93"
        tools:ignore="MissingConstraints" />

    <TextView
        android:id="@+id/state2"
        android:layout_width="90dp"
        android:layout_height="25dp"
        android:layout_alignParentLeft="true"
        android:layout_alignParentTop="true"
        android:gravity="top"
        android:text="상태 : 양호"
        android:textColor="@color/black"
        android:textSize="20dp"
        app:layout_constraintBottom_toTopOf="@+id/real2"
        app:layout_constraintEnd_toEndOf="@+id/rectangle_normal"
        app:layout_constraintHorizontal_bias="0.118"
        app:layout_constraintStart_toStartOf="@+id/rectangle_normal"
        app:layout_constraintTop_toTopOf="@+id/rectangle_normal"
        app:layout_constraintVertical_bias="0.333"
        tools:ignore="MissingConstraints" />


    <TextView
        android:id="@+id/prediction2"
        android:layout_width="120dp"
        android:layout_height="25dp"
        android:layout_alignParentLeft="true"
        android:layout_alignParentTop="true"
        android:gravity="top"
        android:text="예측치 : 000"
        android:textColor="@color/black"
        android:textSize="20dp"
        app:layout_constraintBottom_toTopOf="@+id/real2"
        app:layout_constraintEnd_toEndOf="@+id/rectangle_normal"
        app:layout_constraintHorizontal_bias="0.128"
        app:layout_constraintStart_toStartOf="@+id/rectangle_normal"
        app:layout_constraintTop_toBottomOf="@+id/state2"
        app:layout_constraintVertical_bias="0.755"
        tools:ignore="MissingConstraints" />

    <TextView
        android:id="@+id/real2"
        android:layout_width="120dp"
        android:layout_height="25dp"
        android:layout_alignParentLeft="true"
        android:layout_alignParentTop="true"
        android:layout_marginBottom="28dp"
        android:gravity="top"
        android:text="실측치 : "
        android:textColor="@color/black"
        android:textSize="20dp"
        app:layout_constraintBottom_toTopOf="@+id/error_range2"
        app:layout_constraintEnd_toEndOf="@+id/rectangle_normal"
        app:layout_constraintHorizontal_bias="0.132"
        app:layout_constraintStart_toStartOf="@+id/rectangle_normal"
        tools:ignore="MissingConstraints" />

    <TextView
        android:id="@+id/plant_measure"
        android:layout_width="120dp"
        android:layout_height="25dp"
        android:layout_alignParentLeft="true"
        android:layout_alignParentTop="true"
        android:layout_marginStart="110dp"
        android:layout_marginBottom="28dp"
        android:gravity="top"
        android:text="000"
        android:textColor="@color/black"
        android:textSize="20dp"
        app:layout_constraintBottom_toTopOf="@+id/error_range2"
        app:layout_constraintStart_toStartOf="@+id/rectangle_normal"
        tools:ignore="MissingConstraints" />

    <TextView
        android:id="@+id/error_range2"
        android:layout_width="120dp"
        android:layout_height="25dp"
        android:layout_alignParentLeft="true"
        android:layout_alignParentTop="true"
        android:layout_marginBottom="36dp"
        android:gravity="top"
        android:text="오차 범위 : 00"
        android:textColor="@color/black"
        android:textSize="20dp"
        app:layout_constraintBottom_toTopOf="@+id/check2"
        app:layout_constraintEnd_toEndOf="@+id/rectangle_normal"
        app:layout_constraintHorizontal_bias="0.128"
        app:layout_constraintStart_toStartOf="@+id/rectangle_normal"
        tools:ignore="MissingConstraints" />

    <TextView
        android:id="@+id/check2"
        android:layout_width="300dp"
        android:layout_height="25dp"
        android:layout_alignParentLeft="true"
        android:layout_alignParentTop="true"
        android:layout_marginBottom="24dp"
        android:gravity="top"
        android:text="마지막 점검일 : 0000년 00월 00일"
        android:textColor="@color/black"
        android:textSize="20dp"
        app:layout_constraintBottom_toTopOf="@+id/check_application"
        app:layout_constraintEnd_toEndOf="@+id/rectangle_normal"
        app:layout_constraintHorizontal_bias="0.491"
        app:layout_constraintStart_toStartOf="@+id/rectangle_normal"
        tools:ignore="MissingConstraints" />

    <TextView
        android:id="@+id/check_application"
        android:layout_width="100dp"
        android:layout_height="25dp"
        android:layout_alignParentLeft="true"
        android:layout_alignParentTop="true"
        android:layout_marginBottom="8dp"
        android:gravity="top"
        android:text="점검 신청하기"
        android:textColor="@color/point_check_brown"
        android:textSize="17dp"
        app:layout_constraintBottom_toBottomOf="@+id/rectangle_normal"
        app:layout_constraintEnd_toEndOf="@+id/rectangle_normal"
        app:layout_constraintHorizontal_bias="0.927"
        app:layout_constraintStart_toStartOf="@+id/rectangle_normal"
        tools:ignore="MissingConstraints" />


</androidx.constraintlayout.widget.ConstraintLayout>