<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".Resource_Normal_plant5">


    <ImageView
        android:id="@+id/back_normal_plant5"
        android:layout_width="22dp"
        android:layout_height="29.23dp"
        android:layout_alignParentLeft="true"
        android:layout_alignParentTop="true"
        android:layout_marginStart="16dp"
        android:background="@drawable/back"
        app:layout_constraintBottom_toTopOf="@+id/menu"
        app:layout_constraintEnd_toStartOf="@+id/textView"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/main_toolbar"
        app:layout_constraintVertical_bias="0.022"
        tools:ignore="MissingConstraints" />

    <TextView
        android:id="@+id/textView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentLeft="true"
        android:layout_alignParentTop="true"
        android:layout_marginEnd="256dp"
        android:text="자원관리"
        android:textColor="@color/black"
        android:textSize="30sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toTopOf="@+id/menu"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/main_toolbar"
        app:layout_constraintVertical_bias="0.015"
        tools:ignore="MissingConstraints" />

    <ImageView
        android:id="@+id/powerPlant5"
        android:layout_width="100dp"
        android:layout_height="100dp"
        android:layout_alignParentLeft="true"
        android:layout_alignParentTop="true"
        android:layout_marginTop="64dp"
        android:background="@drawable/img_powerplant_green"
        app:layout_constraintBottom_toTopOf="@+id/PlantName5"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.472"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/main_toolbar"
        tools:ignore="MissingConstraints,UseAppTint" />

    <TextView
        android:id="@+id/PlantName5"
        android:layout_width="100dp"
        android:layout_height="33dp"
        android:layout_alignParentLeft="true"
        android:layout_alignParentTop="true"
        android:layout_marginBottom="52dp"
        android:gravity="center_horizontal|top"
        android:text="88발전소"
        android:textColor="@color/black"
        android:textSize="24sp"
        app:layout_constraintBottom_toTopOf="@+id/rectangle_normal"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.472"
        app:layout_constraintStart_toStartOf="parent"
        tools:ignore="MissingConstraints" />

    <ImageView
        android:id="@+id/rectangle_normal"
        android:layout_width="361dp"
        android:layout_height="323dp"
        android:layout_alignParentLeft="true"
        android:layout_alignParentTop="true"
        android:alpha="0.25"
        android:background="@drawable/img_rectangle_normal"
        app:layout_constraintBottom_toTopOf="@+id/menu"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/main_toolbar"
        app:layout_constraintVertical_bias="0.93"
        tools:ignore="MissingConstraints" />

    <TextView
        android:id="@+id/state5"
        android:layout_width="90dp"
        android:layout_height="25dp"
        android:layout_alignParentLeft="true"
        android:layout_alignParentTop="true"
        android:gravity="top"
        android:text="상태 : 양호"
        android:textColor="@color/black"
        android:textSize="20dp"
        app:layout_constraintBottom_toTopOf="@+id/real5"
        app:layout_constraintEnd_toEndOf="@+id/rectangle_normal"
        app:layout_constraintHorizontal_bias="0.118"
        app:layout_constraintStart_toStartOf="@+id/rectangle_normal"
        app:layout_constraintTop_toTopOf="@+id/rectangle_normal"
        app:layout_constraintVertical_bias="0.333"
        tools:ignore="MissingConstraints" />


    <TextView
        android:id="@+id/prediction5"
        android:layout_width="120dp"
        android:layout_height="25dp"
        android:layout_alignParentLeft="true"
        android:layout_alignParentTop="true"
        android:gravity="top"
        android:text="예측치 : 000"
        android:textColor="@color/black"
        android:textSize="20dp"
        app:layout_constraintBottom_toTopOf="@+id/real5"
        app:layout_constraintEnd_toEndOf="@+id/rectangle_normal"
        app:layout_constraintHorizontal_bias="0.128"
        app:layout_constraintStart_toStartOf="@+id/rectangle_normal"
        app:layout_constraintTop_toBottomOf="@+id/state5"
        app:layout_constraintVertical_bias="0.755"
        tools:ignore="MissingConstraints" />

    <TextView
        android:id="@+id/real5"
        android:layout_width="120dp"
        android:layout_height="25dp"
        android:layout_alignParentLeft="true"
        android:layout_alignParentTop="true"
        android:layout_marginBottom="28dp"
        android:gravity="top"
        android:text="실측치 : 000"
        android:textColor="@color/black"
        android:textSize="20dp"
        app:layout_constraintBottom_toTopOf="@+id/error_range5"
        app:layout_constraintEnd_toEndOf="@+id/rectangle_normal"
        app:layout_constraintHorizontal_bias="0.132"
        app:layout_constraintStart_toStartOf="@+id/rectangle_normal"
        tools:ignore="MissingConstraints" />

    <TextView
        android:id="@+id/error_range5"
        android:layout_width="120dp"
        android:layout_height="25dp"
        android:layout_alignParentLeft="true"
        android:layout_alignParentTop="true"
        android:layout_marginBottom="36dp"
        android:gravity="top"
        android:text="오차 범위 : 00"
        android:textColor="@color/black"
        android:textSize="20dp"
        app:layout_constraintBottom_toTopOf="@+id/check5"
        app:layout_constraintEnd_toEndOf="@+id/rectangle_normal"
        app:layout_constraintHorizontal_bias="0.128"
        app:layout_constraintStart_toStartOf="@+id/rectangle_normal"
        tools:ignore="MissingConstraints" />

    <TextView
        android:id="@+id/check5"
        android:layout_width="300dp"
        android:layout_height="25dp"
        android:layout_alignParentLeft="true"
        android:layout_alignParentTop="true"
        android:layout_marginBottom="24dp"
        android:gravity="top"
        android:text="마지막 점검일 : 0000년 00월 00일"
        android:textColor="@color/black"
        android:textSize="20dp"
        app:layout_constraintBottom_toTopOf="@+id/check_application"
        app:layout_constraintEnd_toEndOf="@+id/rectangle_normal"
        app:layout_constraintHorizontal_bias="0.491"
        app:layout_constraintStart_toStartOf="@+id/rectangle_normal"
        tools:ignore="MissingConstraints" />

    <TextView
        android:id="@+id/check_application"
        android:layout_width="100dp"
        android:layout_height="25dp"
        android:layout_alignParentLeft="true"
        android:layout_alignParentTop="true"
        android:layout_marginBottom="8dp"
        android:gravity="top"
        android:text="점검 신청하기"
        android:textColor="@color/text_check_brown"
        android:textSize="17dp"
        app:layout_constraintBottom_toBottomOf="@+id/rectangle_normal"
        app:layout_constraintEnd_toEndOf="@+id/rectangle_normal"
        app:layout_constraintHorizontal_bias="0.927"
        app:layout_constraintStart_toStartOf="@+id/rectangle_normal"
        tools:ignore="MissingConstraints" />

    <androidx.appcompat.widget.Toolbar
        android:id="@+id/main_toolbar"
        android:layout_width="match_parent"
        android:layout_height="?attr/actionBarSize"
        android:background="@color/point"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent">

        <TextView
            android:id="@+id/toolbar_text"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="center"
            android:layout_marginLeft="20dp"
            android:layout_marginTop="13dp"
            android:layout_marginRight="10dp"
            android:text="SDT"
            android:textColor="#fff"
            android:textSize="20dp"
            android:textStyle="normal" />
    </androidx.appcompat.widget.Toolbar>

    <!--드로어 여는 버튼-->
    <ImageButton
        android:id="@+id/btnOpen"
        android:layout_width="40dp"
        android:layout_height="40dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        android:background="@android:color/transparent"
        android:layout_marginTop="7.5dp"
        android:layout_marginStart="3dp"
        android:scaleType="fitCenter"
        android:src="@drawable/ic_hambuger"/>

    <LinearLayout
        android:id="@+id/menu"
        android:layout_width="0dp"
        android:layout_height="64dp"
        android:background="@color/point"
        android:orientation="horizontal"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent" >

        <ImageButton
            android:id="@+id/homeBtn"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="center"

            android:layout_marginTop="3dp"
            android:layout_weight="1"
            android:backgroundTint="@color/point"
            app:srcCompat="@drawable/ic_home_bottombar"
            tools:ignore="MissingConstraints" />

        <ImageButton
            android:id="@+id/useBtn"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="center"
            android:layout_marginTop="3dp"
            android:layout_weight="1"
            android:backgroundTint="@color/point"
            app:srcCompat="@drawable/ic_consumption_bottombar"
            tools:ignore="MissingConstraints" />

        <ImageButton
            android:id="@+id/dealBtn"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="center"
            android:layout_marginTop="3dp"
            android:layout_weight="1"
            android:backgroundTint="@color/point"
            tools:ignore="MissingConstraints"
            app:srcCompat="@drawable/ic_trade_bottombar" />

        <ImageButton
            android:id="@+id/shareBtn"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="center"
            android:layout_marginTop="3dp"
            android:layout_weight="1"
            android:backgroundTint="@color/point"
            tools:ignore="MissingConstraints"
            app:srcCompat="@drawable/ic_distribution_bottombar" />

        <ImageButton
            android:id="@+id/resourceBtn"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="center"
            android:layout_marginTop="3dp"
            android:layout_weight="1"
            android:backgroundTint="@color/point"
            tools:ignore="MissingConstraints"
            app:srcCompat="@drawable/ic_resource_bottombar" />
    </LinearLayout>

</androidx.constraintlayout.widget.ConstraintLayout>